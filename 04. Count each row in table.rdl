    <DataSet Name="ds_Employee">
      <Query>
        <DataSourceName>ADWDataSource</DataSourceName>
        <rd:DesignerState>
          <QueryDefinition xmlns="http://schemas.microsoft.com/ReportingServices/QueryDefinition/Relational">
            <SelectedColumns>
              <ColumnExpression Schema="Person" ColumnOwner="Person" ColumnName="FirstName" />
              <ColumnExpression Schema="Person" ColumnOwner="Person" ColumnName="LastName" />
              <ColumnExpression Schema="Person" ColumnOwner="EmailAddress" ColumnName="EmailAddress" />
              <ColumnExpression Schema="HumanResources" ColumnOwner="Employee" ColumnName="HireDate" />
            </SelectedColumns>
            <Joins>
              <Join Type="CROSS JOIN" ColumnOwner="Person" Schema="Person" />
              <Join Type="INNER JOIN" ColumnOwner="EmailAddress" Schema="Person">
                <On>
                  <From ColumnOwner="Person" Schema="Person" ColumnName="BusinessEntityID" />
                  <To ColumnOwner="EmailAddress" Schema="Person" ColumnName="BusinessEntityID" />
                </On>
              </Join>
              <Join Type="INNER JOIN" ColumnOwner="Employee" Schema="HumanResources">
                <On>
                  <From ColumnOwner="Person" Schema="Person" ColumnName="BusinessEntityID" />
                  <To ColumnOwner="Employee" Schema="HumanResources" ColumnName="BusinessEntityID" />
                </On>
              </Join>
            </Joins>
          </QueryDefinition>
        </rd:DesignerState>
        <CommandText>SELECT
  Person.Person.FirstName
  ,Person.Person.LastName
  ,Person.EmailAddress.EmailAddress
  ,HumanResources.Employee.HireDate
FROM
  Person.Person
  INNER JOIN Person.EmailAddress
    ON Person.Person.BusinessEntityID = Person.EmailAddress.BusinessEntityID
  INNER JOIN HumanResources.Employee
    ON Person.Person.BusinessEntityID = HumanResources.Employee.BusinessEntityID</CommandText>
      </Query>
