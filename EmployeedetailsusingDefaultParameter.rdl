  <DataSets>
    <DataSet Name="dsEmployee">
      <Query>
        <DataSourceName>dsAdventureWorks</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Gender">
            <Value>=Parameters!pGender.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT e.BusinessEntityID, e.JobTitle, e.OrganizationLevel, e.Gender, e.HireDate, p.FirstName + ' ' + p.LastName AS FullName, p.PersonType
FROM     HumanResources.Employee AS e INNER JOIN
                  Person.Person AS p ON e.BusinessEntityID = p.BusinessEntityID
WHERE  e.Gender = @Gender </CommandText>
      </Query>
      <Fields>
        <Field Name="BusinessEntityID">
